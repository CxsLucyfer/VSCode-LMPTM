{
  "name": "letmeplaythemusic",
  "displayName": "Let Me Play The Music",
  "description": "Control playback from the popular music sites",
  "homepage": "https://github.com/lanly-dev/VSCode-LMPTM",
  "version": "1.0.0",
  "publisher": "lanly-dev",
  "engines": {
    "vscode": "^1.35.0"
  },
  "extensionKind": "ui",
  "categories": [
    "Other"
  ],
  "keywords": [
    "Music",
    "Playback",
    "Browser",
    "Souncloud",
    "Spotify",
    "Youtube"
  ],
  "icon": "icon/lmptm.png",
  "galleryBanner": {
    "color": "#000f33",
    "theme": "dark"
  },
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "copy-static-assets": "ts-node tasks.ts copy",
    "clean-out-files": "ts-node tasks.ts clean"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.11",
    "@types/shelljs": "^0.8.5",
    "shelljs": "^0.8.3",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3",
    "vscode": "^1.1.36"
  },
  "dependencies": {
    "@types/puppeteer-core": "^1.9.0",
    "karma-chrome-launcher": "^3.1.0",
    "puppeteer-core": "^1.20.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lanly-dev/VSCode-LMPTM.git"
  },
  "bugs": {
    "url": "https://github.com/lanly-dev/VSCode-LMPTM/issues"
  },
  "license": "MIT"
}
