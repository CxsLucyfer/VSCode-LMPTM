{
  "name": "let-me-play-the-music",
  "displayName": "LetMePlayTheMusic",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.35.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "clear-out-files": "ts-node clearOutFiles.ts"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.1",
    "@types/shelljs": "^0.8.5",
    "shelljs": "^0.8.3",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3",
    "vscode": "^1.1.36"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.10.1",
    "@types/puppeteer-core": "^1.9.0",
    "karma-chrome-launcher": "^3.0.0",
    "puppeteer-core": "^1.19.0",
    "which-chrome": "^0.1.1"
  }
}
